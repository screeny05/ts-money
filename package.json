{
  "name": "@screeny05/ts-money",
  "type": "module",
  "sideEffects": false,
  "description": "Maintained and improved version of ts-money.",
  "version": "1.2.0",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      },
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.cts"
      }
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "contributors": [
    "Mathew Cormier <mathew.corm@gmail.com>",
    "Sebastian Langer <sl@scn.cx>"
  ],
  "keywords": [
    "money",
    "javascript",
    "currency",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/screeny05/ts-money.git"
  },
  "bugs": {
    "url": "https://github.com/screeny05/ts-money/issues"
  },
  "devDependencies": {
    "@types/node": "^20.14.15",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vite": "^5.4.1",
    "vitest": "^2.0.5"
  },
  "scripts": {
    "build": "unbuild",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  }
}
